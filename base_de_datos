CREATE DATABASE tesis;

-- Usar la base de datos
USE tesis;

-- Crear la tabla para almacenar los valores de SpO2 e IR
CREATE TABLE oximetro (
    id INT AUTO_INCREMENT PRIMARY KEY,   -- Identificador único para cada registro
    spO2 FLOAT NOT NULL,                 -- Valor de SpO2 (Saturación de oxígeno en sangre)
    ir FLOAT NOT NULL,                   -- Valor de IR (Infrarrojo)
    created_at TIMESTAMP DEFAULT CURRENT_TIMESTAMP  -- Fecha y hora de inserción del dato
);
